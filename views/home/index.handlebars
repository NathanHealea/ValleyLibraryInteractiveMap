<!DOCTYPE html>
<html lang="ab">
<head>
    {{> head }}
</head>
</html>
<body>
    <div id="wrapper">
        {{> nav }}
        {{> home/sidebar }}

        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <button class="btn" id="loadGrid">Load Grid</button>
                        <button style="display:none"  class="btn" id="navLine">Nav Line</button>
                        <button style="display:none" class="btn" id="setWalkTrue">Set Walkable</button>
                        <button style="display:none" class="btn" id="setWalkFalse">Set Unwalkable</button>
                        <button style="display:none" class="btn" id="hideGrid">Hide Grid</button>
                        <button style="display:none" class="btn" id="saveGrid">Save Grid</button>
                        <div id="map-group" style=" display:flex">
                            <object type="image/svg+xml" id="map-wrapper" data="/public/images/map-floor-1.svg" style="pointer-events:all;">
                                Your browser does not support SVG
                            </object>
                            <section id="grid" style="position:absolute"></section>
                        </div> <!-- / #map-group !-->

                        <div id="mapDiv" style="display:flex">

                        </div>

                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->
        {{> footer }}
    </div>
    <!-- /#wrapper -->

    {{> home/javascript }}

    <!-- MetisMenu js -->
    <script src='/metismenu/metisMenu.min.js'></script>

    <!-- Dashboard js -->
    <script src='/public/javascripts/dashboard-admin.js'></script>

    <!-- Pathfinding js-->
    <script src="/pathfinding/visual/lib/pathfinding-browser.min.js"></script>

    <!-- Navigation js -->
    <script src="/public/javascripts/navigation-main.js"></script>

    
    <!--polygon-hover.js-->
    <script src="/public/javascripts/polygon-hover.js"></script>

    <!-- D3 js -->
    <script src="/d3/build/d3.min.js"></script>

    <!-- Lodash js -->
    <script src="lodash/lodash.js"></script>

    <script>
        $.ajax({
            type: "GET",
            async: true,
            url: '/home/location'
        })
                .done(function (data) {
                    console.log(data);
                    var result = JSON.parse(data);

                })
                .fail(function () {
                    console.log("Ajax Failed.");
                });
    </script>


</body>